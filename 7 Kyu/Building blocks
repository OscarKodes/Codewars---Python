class Block:
    
    def __init__(self, l):
        
        [self.w, self.l, self.h] = l
        
    get_width = lambda b: b.w
    get_length = lambda b: b.l
    get_height = lambda b: b.h
    get_volume = lambda b: b.w * b.l * b.h
    get_surface_area = lambda b: (b.w * b.l + b.w * b.h + b.l * b.h) * 2
