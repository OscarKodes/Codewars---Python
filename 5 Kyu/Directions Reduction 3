def dir_reduc(arr):
    
    result = arr[:]
    opp = {"WEST":"EAST","EAST":"WEST","NORTH":"SOUTH","SOUTH":"NORTH"}

    i = 0
    while i < len(result) - 1:

        if result[i] == opp[result[i+1]]:
            result = result[:i] + result[i+2:]
            i = 0 if i == 0 else i - 1

        else:
            i += 1    

    return result
